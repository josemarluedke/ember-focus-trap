{
  "name": "ember-focus-trap",
  "version": "1.0.2",
  "description": "A Ember modifier to trap your focus.",
  "keywords": [
    "ember-addon"
  ],
  "homepage": "https://josemarluedke.github.io/ember-focus-trap",
  "repository": "https://github.com/josemarluedke/ember-focus-trap",
  "license": "MIT",
  "author": "Josemar Luedke <josemarluedke@gmail.com>",
  "files": [
    "addon-main.js",
    "dist"
  ],
  "scripts": {
    "build": "rollup --config",
    "start": "rollup --config --watch",
    "test": "echo 'Addon does not have tests, run tests in test-app'",
    "prepare": "yarn build"
  },
  "dependencies": {
    "@embroider/addon-shim": "^1.0.0",
    "focus-trap": "^6.7.1"
  },
  "devDependencies": {
    "@babel/core": "7.16.7",
    "@babel/plugin-proposal-class-properties": "7.16.7",
    "@embroider/addon-dev": "^1.0.0",
    "@rollup/plugin-babel": "5.3.0",
    "npm-run-all": "4.1.5",
    "rollup": "2.66.1"
  },
  "engines": {
    "node": "12.* || >= 14"
  },
  "publishConfig": {
    "access": "public"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "version": 2,
    "type": "addon",
    "main": "addon-main.js",
    "app-js": {
      "./modifiers/focus-trap.js": "./dist/_app_/modifiers/focus-trap.js"
    }
  },
  "exports": {
    "./*": "./dist/*",
    "./addon-main.js": "./addon-main.js"
  }
}
