{
  "name": "ember-focus-trap",
  "version": "1.0.2",
  "description": "A Ember modifier to trap your focus.",
  "keywords": [
    "ember-addon"
  ],
  "homepage": "https://josemarluedke.github.io/ember-focus-trap",
  "repository": "https://github.com/josemarluedke/ember-focus-trap",
  "license": "MIT",
  "author": "Josemar Luedke <josemarluedke@gmail.com>",
  "files": [
    "addon-main.js",
    "dist"
  ],
  "scripts": {
    "build": "rollup --config",
    "start": "rollup --config --watch",
    "test": "echo 'Addon does not have tests, run tests in test-app'",
    "prepare": "yarn build"
  },
  "peerDependencies": {
    "ember-source": "^4.0.0 || ^5.0.0"
  },
  "dependencies": {
    "@embroider/addon-shim": "^1.8.6",
    "focus-trap": "^7.5.2"
  },
  "devDependencies": {
    "@babel/core": "7.22.10",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@embroider/addon-dev": "^4.1.0",
    "@rollup/plugin-babel": "6.0.3",
    "ember-source": "^5.2.0",
    "npm-run-all": "4.1.5",
    "rollup": "3.28.0"
  },
  "engines": {
    "node": "12.* || >= 14"
  },
  "publishConfig": {
    "access": "public"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "version": 2,
    "type": "addon",
    "main": "addon-main.js",
    "app-js": {
      "./modifiers/focus-trap.js": "./dist/_app_/modifiers/focus-trap.js"
    }
  },
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*",
    "./addon-main.js": "./addon-main.js"
  }
}
