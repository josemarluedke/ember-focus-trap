{
  "name": "ember-focus-trap",
  "version": "1.1.0",
  "description": "A Ember modifier to trap your focus.",
  "keywords": [
    "ember-addon"
  ],
  "homepage": "https://josemarluedke.github.io/ember-focus-trap",
  "repository": "https://github.com/josemarluedke/ember-focus-trap",
  "license": "MIT",
  "author": "Josemar Luedke <josemarluedke@gmail.com>",
  "files": [
    "addon-main.js",
    "dist"
  ],
  "scripts": {
    "build": "rollup --config",
    "start": "rollup --config --watch",
    "test": "echo 'Addon does not have tests, run tests in test-app'",
    "prepare": "pnpm build"
  },
  "peerDependencies": {
    "ember-source": ">= 4.0.0"
  },
  "dependencies": {
    "@embroider/addon-shim": "^1.9.0",
    "focus-trap": "^6.7.1"
  },
  "devDependencies": {
    "@babel/core": "7.26.0",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@embroider/addon-dev": "^7.0.0",
    "@rollup/plugin-babel": "6.0.4",
    "npm-run-all": "4.1.5",
    "rollup": "4.26.0",
    "webpack": "^5.90.3"
  },
  "engines": {
    "node": "12.* || >= 14"
  },
  "publishConfig": {
    "access": "public"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "version": 2,
    "type": "addon",
    "main": "addon-main.js",
    "app-js": {
      "./modifiers/focus-trap.js": "./dist/_app_/modifiers/focus-trap.js"
    }
  },
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*",
    "./addon-main.js": "./addon-main.js"
  }
}
